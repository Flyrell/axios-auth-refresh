{
    "name": "axios-auth-refresh",
    "version": "3.3.4",
    "description": "Axios plugin which makes it very easy to automatically refresh the authorization tokens of your clients",
    "keywords": [
        "axios",
        "authentication",
        "authorization",
        "refresh token",
        "access token",
        "interceptor",
        "auto refresh"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": "https://github.com/Flyrell/axios-auth-refresh",
    "author": "Dawid Zbi≈Ñski <dawid@zbinski.eu>",
    "license": "MIT",
    "private": false,
    "scripts": {
        "build": "rollup --config rollup.config.ts --configPlugin typescript",
        "typescript": "tsc --noEmit",
        "lint": "eslint .",
        "lint:fix": "yarn lint --fix",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "prettier": "prettier --write .",
        "prettier:staged": "pretty-quick --staged",
        "prepare": "husky install"
    },
    "lint-staged": {
        "*": "prettier --ignore-unknown --write",
        "*.(js|jsx|ts|tsx)": "eslint --fix"
    },
    "peerDependencies": {
        "axios": ">= 0.18 < 0.19.0 || >= 0.19.1"
    },
    "devDependencies": {
        "@rollup/plugin-typescript": "^9.0.1",
        "@types/jest": "^29.1.2",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "axios": "^0.27.2",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-simple-import-sort": "^8.0.0",
        "husky": "^8.0.1",
        "jest": "^29.1.2",
        "prettier": "2.7.1",
        "rollup": "^3.2.3",
        "rollup-plugin-terser": "^7.0.2",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    },
    "files": [
        "dist",
        "LICENSE"
    ]
}
